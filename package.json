{
  "name": "blog-app",
  "version": "1.0.0",
  "description": "Application de blog avec Next.js en frontend et Express en backend",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon blog-backend/server.js",
    "client": "cd frontend && npm run dev",
    "server": "nodemon blog-backend/server.js",
    "dev:fullstack": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^2.0.0",
    "next": "^14.1.0",
    "quill": "^2.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-quill": "^2.0.0",
    "swr": "^2.3.3",
    "validator": "^13.15.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.4"
  },
  "keywords": [
    "blog",
    "express",
    "nextjs",
    "mongodb",
    "react"
  ],
  "author": "Ton Nom <ton@email.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
